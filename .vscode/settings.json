{
    "cmake.buildDirectory": "${workspaceFolder}/Build",
    "clangd.arguments": [],
    "clangd.fallbackFlags": [
        "-I${workspaceFolder}/Include",
        "-I${workspaceFolder}"
    ],
    "Codegeex.RepoIndex": true,
    "cmake.ignoreCMakeListsMissing": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.cache": true,
        "**/.xmake": true,
        "**/.vscode": true
    },
    "explorerExclude.backup": {}
}
